{
    "name": "Battery Digital Twin Model Suite",
    "version": "2.0.0",
    "main": "entrypoint.py",
    "type": "model",
    "category": "Performance",
    "parameters": {
        "execution": {
            "enabled_models": {
                "description": "Comma-separated list of models to enable (eq_cycles, battery_usage_analyzer)",
                "default": "eq_cycles,battery_usage_analyzer"
            },
            "compute_type": {
                "description": "Compute mode: 'incremental/full/manual loads existing output data ",
                "default": "manual"
            },
            "max_days_period_compute": {
                "description": "The number of days that the model will compute in one execution",
                "default": 1
            },
            "manual_start_date": {
                "description": "Manual start date for data slice in ISO 8601 format with timezone (YYYY-MM-DDTHH:MM:SS±HH:MM). Leave empty to use automatic date calculation",
                "default": "2023-10-16T00:18:04+08:00"
            },
            "manual_end_date": {
                "description": "Manual end date for data slice in ISO 8601 format with timezone (YYYY-MM-DDTHH:MM:SS±HH:MM). Leave empty to use automatic date calculation",
                "default": "2023-10-16T18:59:59+00:00"
            },
            "debug_mode": {
                "description": "Enable debug dashboard generation for this model",
                "default": true
            },
            "upload_output": {
                "description": "Enable output upload to altergo platform",
                "default": false
            }
        },
        "models": {
            "eq_cycles": {
                "inputs": {
                    "current": {
                        "default": "Current"
                    },
                    "soh": {
                        "default": "SOH"
                    },
                    "capacity": {
                        "default": "Capacity"
                    },
                    "initial_equivalent_cycles": {
                        "default": "Equivalent Cycles"
                    },
                    "OCV_table": {
                        "default": "BP/LFP_config"
                    }
                },
                "outputs": {
                    "equivalent_cycles": {
                        "default": "Equivalent Cycles"
                    }
                },
                "configuration": {
                }
            },
            "battery_usage_analyzer": {
                "inputs": {
                    "current": {
                        "default": "Current"
                    },
                    "soc": {
                        "default": "SoC"
                    },
                    "v_cell_min": {
                        "default": "Minimum Voltage"
                    },
                    "v_cell_max": {
                        "default": "Maximum Voltage"
                    },
                    "t_cell_min": {
                        "default": "Temp Cell Min"
                    },
                    "t_cell_max": {
                        "default": "Temp Cell Max"
                    }
                },
                "outputs": {
                    "seg_l0_id": {
                        "default": "Operating_Mode_Segment_ID"
                    },
                    "mode": {
                        "default": "Operating_Mode"
                    },
                    "boundary_l1": {
                        "default": "Change_Point_Boundary"
                    },
                    "seg_l1_id": {
                        "default": "Change_Point_Segment_ID"
                    },
                    "phase": {
                        "default": "Battery_Phase"
                    },
                    "seg_l2_id": {
                        "default": "Phase_Segment_ID"
                    }
                },
                "configuration": {
                    "window_s": 5.0,
                    "i_rest_th_A": 0.5,
                    "i_charge_on_A": 2.0,
                    "i_discharge_on_A": 2.0,
                    "min_state_duration_s": 60.0,
                    "min_seg_duration_s": 120.0,
                    "min_phase_duration_s": 60.0,
                    "change_quantile": 0.97,
                    "enable_rainflow": false
                }
            }
        }
    }
}